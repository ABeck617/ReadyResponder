<%= simple_form_for(@resource_type) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :status %>
    <%= f.input :description %>
    <%= f.input :fema_code %>
    <%= f.input :fema_kind %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
